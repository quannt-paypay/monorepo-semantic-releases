{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "start": {
      "cache": false
    },
    "release": {
      "dependsOn": ["^release"],
      "outputMode": "new-only"
    },
    "release-pre": {
      "dependsOn": ["^release:pre"],
      "outputMode": "new-only"
    }
  }
}
